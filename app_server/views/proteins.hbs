     Description: Template for the Proteins webpage integrating header and footer partials -->

<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>About - Travlr Getaways Web Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header_about }}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Proteins</h1>
							<ul id="sites">
			{{#each proteins}}
								<li>
									<a href="/api/proteins/{{this.Ensembl}}"><img src="images/{{this.image}}" alt="Img"></a>
									<h2><a href="/proteins">{{this.Gene}}</a></h2>
									{{{this.Description}}}
								</li>
			{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer_about }}
	</div>
</body>
</html>